ript>\r\nimport vuescroll from \"vuescroll\";\r\nimport Article from \"@/components/article/Article\";\r\nexport default {\r\n  components: {\r\n    Article,\r\n    vuescroll,\r\n  },\r\n  data() {\r\n    return {\r\n      articles: [],\r\n      ops: {\r\n        vuescroll: {},\r\n        scrollPanel: {},\r\n        rail: {\r\n          background: \"#01a99a\",\r\n          opacity: 0,\r\n          size: \"6px\",\r\n          specifyBorderRadius: false,\r\n          gutterOfEnds: null,\r\n          gutterOfSide: \"2px\",\r\n          keepShow: false,\r\n        },\r\n        bar: {\r\n          showDelay: 500,\r\n          onlyShowBarOnScroll: true,\r\n          keepShow: false,\r\n          background: \"cyan\",\r\n          opacity: 1,\r\n          hoverStyle: false,\r\n          specifyBorderRadius: false,\r\n          minSize: 0,\r\n          size: \"6px\",\r\n          disable: false,\r\n        },\r\n      },\r\n    };\r\n  },\r\n  computed: {},\r\n  async created() {\r\n    let result = await this.$http({ type: \"articles\" });\r\n    console.log(result, \"result\");\r\n    result = result.data.data.list.map((item) => {\r\n      // console.log(item.content.match(/>([^<>]+)</),\"matched\");\r\n      item.content = item.content.match(/>([^<>]+)</)[1];\r\n      return item;\r\n    });\r\n\r\n    this.articles = result;\r\n  },\r\n  mounted() {},\r\n\r\n  methods: {\r\n    async handleScroll(vertical, horizontal, nativeEvent) {\r\n      let sh = vertical.scrollTop\r\n      let vh = nativeEvent.srcElement.clientHeight\r\n      let allH = nativeEvent.srcElement.scrollHeight\r\n      if (vh + sh + 200 > allH) {\r\n        let result = await this.$http({type: \"articles\"})\r\n        console.log(result.data.data.list);\r\n        this.articles.push(...result.data.data.list)\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"stylus\">\r\nbody .blog-page .blog-scroll--wrap {\r\n  height: 60vh;\r\n  background-color: #9a9a9a;\r\n}\r\n</style>"]}]}