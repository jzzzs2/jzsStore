">\n          <el-col :span=\"8\" class=\"hidden-xs-only\">\n            <el-aside class=\"blog-content--aside\">\n              <Aside v-if=\"isUserExist\" />\n            </el-aside>\n          </el-col>\n          <el-col :span=\"16\">\n            <el-main class=\"blog-content--main\">\n              <router-view></router-view>\n            </el-main>\n          </el-col>\n        </el-row>\n      </el-container>\n      <el-footer height=\"10vh\" class=\"blog-footer\">底部</el-footer>\n    </el-container>\n    <Modal />\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport b from \"@/config/http.config\";\nimport store from \"store\";\nlet { TOKEN_NAME } = b;\nimport Modal from \"./Modal.vue\";\nimport Header from \"@/components/header/Header.vue\";\nimport Aside from \"@/components/aside/AsideUserInfo\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {};\n  },\n  computed: {\n    isUserExist() {\n      return this.$store.state.token;\n    },\n  },\n  mounted() {\n    if (store.get(TOKEN_NAME)) {\n      this.$http({ type: \"index\" })\n        .then((result) => {\n          this.$store.dispatch(\"setUser\", result.data.data);\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    }\n  },\n  components: {\n    Modal,\n    Header,\n    Aside,\n  },\n};\n</script>\n<style lang=\"stylus\">\n@import '~@/assets/css/variable.styl';\n.blog-content-wrap \n  height: 80vh;\n  background-color page-main-color\n  & .el-row\n    width: 100%\n\n.blog-content--main\n  margin-top 80px\n  height: 60vh\n\n.blog-footer {\n  background-color: #222;\n}\n\n.blog-header {\n  background-color: #999;\n  padding: 0;\n}\n\n.blog-header--row {\n  height: 100%;\n  align-items: center;\n}\n\n.blog-page .blog-content-wrap .blog-content--aside\n  margin: 100px auto\n  width: 80%!important\n</style>\n"]}]}