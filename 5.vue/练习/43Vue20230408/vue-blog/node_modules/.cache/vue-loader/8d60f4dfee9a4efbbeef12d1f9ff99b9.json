sets/img/xiangcai.png')\"\n              fit=\"cover\"\n            ></el-image>\n          </el-col>\n          <el-col :span=\"16\" class=\"hidden-xs-only\">\n            <!-- <el-menu mode=\"horizontal\" active-text-color=\"#fff\" background-color=\"#222\"> -->\n            <el-menu mode=\"horizontal\" router active-text-color=\"#409EFF\">\n              <el-menu-item index=\"/\">主页</el-menu-item>\n              <el-menu-item index=\"/column\">分类</el-menu-item>\n              <el-menu-item index=\"/article/list\">文章列表</el-menu-item>\n              <el-menu-item index=\"/article\">其他</el-menu-item>\n              <el-submenu index=\"2\">\n                <template slot=\"title\">外链</template>\n                <!-- <el-menu-item index=\"http://baidu.com/\">baidu</el-menu-item>\n            <el-menu-item index=\"http://alibaba.com/\">alibaba</el-menu-item> -->\n                <el-menu-item index=\"/\">baidu</el-menu-item>\n                <el-menu-item index=\"/\">alibaba</el-menu-item>\n              </el-submenu>\n            </el-menu>\n          </el-col>\n          <component :is=\"isUser\"></component>\n        </el-row>\n      </el-header>\n      <el-container class=\"blog-content-wrap\">\n        <el-aside class=\"blog-content--aside\">侧边</el-aside>\n        <el-main class=\"blog-content--main\">\n          <router-view></router-view>\n        </el-main>\n      </el-container>\n      <el-footer height=\"10vh\" class=\"blog-footer\">底部</el-footer>\n    </el-container>\n    <Modal />\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport b from \"@/config/http.config\";\nlet { TOKEN_NAME } = b;\nimport Modal from \"./Modal.vue\";\nimport NormalUser from \"@/components/user/NormalUser\";\nimport UserAvatar from \"@/components/user/UserAvatar\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {};\n  },\n  mounted() {\n    if (stote.get(TOKEN_NAME)) {\n      this.$http({ type: \"index\" })\n        .then((result) => {\n          this.$store.dispatch(\"changeUser\", result.data);\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    }\n  },\n  components: {\n    Modal,\n    NormalUser,\n    UserAvatar,\n  },\n  computed: {\n    isUser() {\n      return this.$store.state.token ? \"UserAvatar\" : \"NormalUser\";\n    },\n  },\n};\n</script>\n<style lang=\"stylus\">\n.blog-content-wrap {\n  height: 80vh;\n}\n\n.blog-content--aside {\n  background-color: cyan;\n}\n\n.blog-content--main {\n  background-color: yellow;\n}\n\n.blog-footer {\n  background-color: #222;\n}\n\n.blog-header {\n  background-color: #999;\n  padding: 0;\n}\n\n.blog-header--row {\n  height: 100%;\n  align-items: center;\n}\n</style>\n"]}]}