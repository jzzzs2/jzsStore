icleList.vue\"\nimport ArticleSingle from \"@/components/article/ArticleSingle.vue\"\nimport ArticleEdit from \"@/components/article/ArticleEdit.vue\"\nimport Column from \"@/views/Column.vue\"\nimport MyInfo from \"@/views/MyInfo.vue\"\nimport NProgress from \"nprogress\"\nimport Photo from \"@/views/Photo.vue\"\nimport Wechat from \"@/views/Wechat.vue\"\nimport \"nprogress/nprogress.css\"\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/',\n    name: 'Home',\n    component: Home,\n    redirect: \"/articles\",\n    children: [\n      {\n        path: \"/articles\",\n        component: ArticleList,\n        name: \"articles\"\n      },\n      {\n        path: \"/article/:id\",\n        component: ArticleSingle,\n        name: \"singleArticle\"\n      },\n      {\n        path: \"/column\",\n        component: Column,\n        name: \"column\",\n        meta: {\n          needAuth: true\n        }\n      },\n      {\n        path: \"/edit\",\n        component: ArticleEdit,\n        name: \"Edit\",\n        props: (route) => {\n          return {\n            initClassifyId: route.query.classifyId\n          }\n        }\n      },\n      {\n        path: \"/userInfo\",\n        component: MyInfo,\n        name: \"myInfo\"\n      },\n      {\n        path: \"/photo\",\n        component: Photo,\n        name: \"photo\"\n      },\n      {\n        path: '/wechat',\n        component: Wechat,\n        name: \"wechar\"\n      }\n    ]\n  }\n]\n\nconst router = new VueRouter({\n  routes\n})\nrouter.beforeEach( (to,from,next) => {\n  NProgress.start()\n  console.log(to,from);\n  if (to.meta.needAuth) {\n    NProgress.inc()\n    if(!vuex.state.token) {\n      Vue.prototype.$notify({\n        title: '权限不足',\n        message: '请先登入',\n        type: 'error'\n      })\n      NProgress.done()\n      next(\"/\")\n    }\n  }\n  next()\n})\nrouter.afterEach( (to,from) => {\n  console.log(to,from);\n  NProgress.done()\n})\nexport default router\n"],"mappings":"AAAA;AACA;AAC