<div class="blog-modal-wrap">
  <div class="blog-modal-head">
    <h3>{{title}}</h3>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close" data-btn-type="close"><span
        aria-hidden="true" data-btn-type="close">&times;</span></button>
  </div>
  <div class="blog-modal-content">
    <form class="form-horizontal" action="#">
      {{#each formData}}
      <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">{{name}}</label>
        <div class="col-sm-6">
          <input type="{{inputType}}" class="form-control"placeholder="{{placeHolder}}">
        </div>
      </div>
      {{/each}}
      <div class="form-group">
        {{#each btns}}
        <button type="submit" class="btn btn-default" data-btn-type="{{type}}">{{resName}}</button>
        {{/each}}
      </div>
    </form>
  </div>
</div>
